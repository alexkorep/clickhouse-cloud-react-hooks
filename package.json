{
  "name": "clickhouse-cloud-react-hooks",
  "private": false,
  "version": "0.1.0",
  "description": "A collection of reusable React hooks to easily interact with the ClickHouse Cloud API.",
  "type": "module",
  "main": "./dist/clickhouse-cloud-react-hooks.umd.js",
  "module": "./dist/clickhouse-cloud-react-hooks.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/clickhouse-cloud-react-hooks.es.js",
      "require": "./dist/clickhouse-cloud-react-hooks.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "publish": "npm run build && npm run test && npm publish",
    "example": "cd example && yarn dev",
    "example:install": "cd example && yarn install",
    "example:build": "cd example && yarn build"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "swr": "^2.3.4",
    "zod": "^4.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
